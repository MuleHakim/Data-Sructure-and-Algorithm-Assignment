def quickSort(lst):
    if len(lst) <= 1:
        return lst
    else:
        pivot = lst.pop()
        lower = []
        greater = []
        for i in lst:
            if i > pivot:
                greater.append(i)
            else:
                lower.append(i)
    return quickSort(lower) + [pivot] + quickSort(greater)
print(quickSort([0,9,3,8,2,7,5]))